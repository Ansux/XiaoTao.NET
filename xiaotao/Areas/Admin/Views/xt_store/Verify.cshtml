@model IEnumerable<xiaotao.Models.xt_store>

@{
   ViewBag.Title = "Index";
   ViewBag.Controller = "申请核验";
   ViewBag.Action = "列表";
}

<table class="table">
   <tr class="active">
      <th>
         @Html.DisplayNameFor(model => model.login_id)
      </th>
      <th>
         @Html.DisplayNameFor(model => model.avatar)
      </th>
      <th>
         @Html.DisplayNameFor(model => model.name)
      </th>
      <th>
         @Html.DisplayNameFor(model => model.email)
      </th>
      <th>
         @Html.DisplayNameFor(model => model.phone)
      </th>
      <th>
         @Html.DisplayNameFor(model => model.create_at)
      </th>
      <th>
         @Html.DisplayNameFor(model => model.verify)
      </th>
      <th>
         @Html.DisplayNameFor(model => model.states)
      </th>
      <th>操作</th>
   </tr>

   @foreach (var item in Model)
   {
      <tr>
         <td>
            @Html.DisplayFor(modelItem => item.login_id)
         </td>
         <td>
            @Html.DisplayFor(modelItem => item.avatar)
         </td>
         <td>
            @Html.DisplayFor(modelItem => item.name)
         </td>
         <td>
            @Html.DisplayFor(modelItem => item.email)
         </td>
         <td>
            @Html.DisplayFor(modelItem => item.phone)
         </td>
         <td>
            @Html.Raw(item.create_at.ToShortDateString())
         </td>
         <td>
            @Html.DisplayFor(modelItem => item.verify)
         </td>
         <td>
            @Html.DisplayFor(modelItem => item.states)
         </td>
         <td>
            @Html.ActionLink("核验", "Check", new { id = item.id })
         </td>
      </tr>
   }

</table>